{
	"$schema": "http://json.schemastore.org/prettierrc",
	"printWidth": 80,
	"endOfLine": "lf",
	"useTabs": true,
	"arrowParens": "always",
	"quoteProps": "consistent",
	"bracketSpacing": true,
	"singleQuote": false,
	"semi": true,
	"trailingComma": "es5",
	"htmlWhitespaceSensitivity": "ignore",
	"overrides": [
		{
			"files": "*.html",
			"options": { "parser": "angular" }
		}
	]
}
